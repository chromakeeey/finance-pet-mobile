{
  "name": "financeapp",
  "version": "0.0.1",
  "license": "MIT",
  "private": true,
  "scripts": {
    "// --- develop --- //": "",
    "bootstrap": "yarn && cd ios && npx pod install && cd ../",
    "dev": "yarn adb && react-native start",
    "adb": "(adb reverse tcp:8081 tcp:8081 || true) && (adb reverse tcp:7007 tcp:7007 || true)",
    "android": "adb shell pm uninstall --user 0 com.tasa ; react-native run-android",
    "ios": "react-native run-ios --simulator='iPhone 14 Pro",
    "// --- build --- //": "",
    "// --- test --- //": "",
    "test": "jest --passWithNoTests",
    "// --- tools --- //": "",
    "pull": "git pull origin main",
    "merge": "git merge origin main",
    "pods": "cd ios && pod install && cd ..",
    "pods:m1": "cd ios && arch -x86_64 pod install && cd ..",
    "android:clear": "cd android && ./gradlew clean && cd ..",
    "ios:clear": "cd ios && rm -rf ./Podfile.lock && rm -rf ./Pods && cd ..",
    "// --- linters and formatters --- //": "",
    "lint": "eslint src/** --fix --no-error-on-unmatched-pattern",
    "format": "prettier --write '**/*.(ts|tsx)'",
    "validate": "tsc && yarn lint && pretty-quick --staged",
    "prepare": "is-ci || (npx husky install && pretty-quick --staged)"
  },
